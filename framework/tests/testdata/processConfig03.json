{
    "processName" : "answer hello world to an http GET request",
    "pipelines" : [
        {
            "pipelineConfigFile":"pipelineConfig08.json",
            "matchingPatterns" : [
                {
                    "http.rcv.headerField.method": "GET",
                    "http.rcv.headerField.path": "/api/1.0",
                    "http.rcv.urlParameter.what": "helloworld"
                }
            ]
        }
    ]
}